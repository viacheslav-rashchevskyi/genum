{
  "name": "core",
  "private": true,
  "version": "1.1.0-beta.2",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build:alias": "tsc-alias",
    "build:full": "npm run build && npm run build:alias",
    "type-check": "tsc --noEmit",
    "start": "node dist/server.js",
    "dev": "dotenv -e ../../.env tsx watch src/server.ts",
    "seed:dev": "tsx src/database/seed/seed.ts",
    "seed:prod": "node dist/database/seed/seed.js",
    "db:generate": "prisma generate",
    "dev:db-init": "npm run db:generate && dotenv -e ../../.env prisma migrate dev && dotenv -e ../../.env npm run seed:dev",
    "db-init": "prisma migrate deploy && npm run seed:prod",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.6",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.1",
    "@vitest/ui": "^4.0.17",
    "nodemon": "^3.1.11",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^6.0.4",
    "vitest": "^4.0.17"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.58.0",
    "@clickhouse/client": "^1.12.1",
    "@google/genai": "^1.4.0",
    "@langchain/anthropic": "^1.3.7",
    "@langchain/core": "^1.1.12",
    "@langchain/google-genai": "^2.1.7",
    "@langchain/langgraph": "^1.0.15",
    "@langchain/openai": "^1.2.1",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@sentry/node": "^10.30.0",
    "axios": "^1.7.9",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^5.2.1",
    "express-oauth2-jwt-bearer": "^1.7.1",
    "langchain": "^1.2.7",
    "moment": "^2.30.1",
    "multer": "2.0.2",
    "openai": "^6.10.0",
    "prisma": "^7.2.0",
    "prisma-zod-generator": "^2.1.2",
    "tsx": "^4.20.3",
    "zod": "^4.1.13"
  }
}
